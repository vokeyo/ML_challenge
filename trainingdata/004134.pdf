		function search(terms) {
		    location = "search.aspx?q=" + terms;
			document.getElementById("results").innerHTML = "<center><img src='Images/2T_loadinfo_spinner.gif' /><br/>Searching...</center>";
		//	eDirectives.Presentation.Resources.Ajax.AjaxSearch.processSearch(terms, search_cb);
		}
		
		function search_cb(response) {
		    alert(response.value[1]);
		    if (response.value[1])
		    {
			    document.getElementById("results").innerHTML = response.value[0];
			    document.getElementById("qs_ea").innerHTML  = "<img src=\"Images/navigation/section_search_results.jpg\">";
			    document.getElementById("qs_ea").style.display = 'inline';
			    document.getElementById("qs_ea").style.backgroundColor = "#1B7C1B";
			    document.getElementById("breadcrumb").innerHTML = "<a href='Default.aspx'>Home</a> / Search Results";
			}
			//id_list = response.value[1];
		}
		function navigate_cb(response) {
			if (response.error != null) {
				alert(response.error);
			}
			document.getElementById("searchResults").innerHTML = response.value[0];
			document.getElementById("topNav").innerHTML = response.value[3];
			document.getElementById("bottomNav").innerHTML = response.value[3];
			document.getElementById("returnStatus").innerHTML = "showing page "+ response.value[1] +" of "+ response.value[4] +" ("+ response.value[5] +" total matches found)";
		}
		
		function open_window(iurl) {
			if (window.misc_win) {
				misc_win.close();
			}
			misc_win = window.open(iurl,"misc_win",'toolbar=0,location=0,directories=0,status=0,menubar=1,scrollbars=1,resizable=1,width=800,height='+ (window.screen.availHeight - (window.screen.availHeight * .4)));
		}
		function dn_submit() {
			if (window.event && window.event.keyCode == 13) {
				search(document.search_form.SearchText.value);
				return;
			}
		}
        
        
        function node_exists(wnId,lvl,objTimer)  {
            if (document.getElementById('to' + wnId))  {
                expand_tree(wnId,lvl);
                clearInterval(objTimer);
            }
        }
        		
		
		
