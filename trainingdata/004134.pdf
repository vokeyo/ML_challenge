			var selObj = new Image;
			
			function home_deselected() {
				document.navhome.src = 'Images/navigation/top_nav_home_off.gif';
			}
			
			function view_directive(id){
				open_window('viewerFS.aspx?hid='+ id);
			}
			
			function setFav(url, title) {
				alert(url);
				alert(title);
				window.external.AddFavorite(url, title)
			}
			
			function expand_main(id, lvl, img) {
				home_deselected();
				if (lvl == 1) {
					document.getElementById('breadcrumb').innerHTML = '';
					document.getElementById('results').innerHTML = '<p>&nbsp;&nbsp;&nbsp;&nbsp;loading content...';
					eDirectives.Presentation.Resources.Ajax.AjaxMethods.getBreadcrumb(id, show_breadcrumb);
				}	
				document.getElementById('results').style.display = 'inline';
				document.getElementById('dir_disp').style.display = 'none';
				document.getElementById("qs_ea").innerHTML = "<img src=\"Images/navigation/section"+ id +".jpg\">";
				document.getElementById("qs_ea").style.display = 'inline';

				if (img != selObj) {
					selObj.src = selObj.src.replace('_sel', '_off');
					selObj = img;
					selObj.src = selObj.src.replace('_on', '_sel');
					selObj.src = selObj.src.replace('_off', '_sel');	
				}
				eDirectives.Presentation.Resources.Ajax.AjaxMethods.getChildList(id, parseInt(lvl), expand_main_cb);
			}
			
			function expand_main_cb(response) {
				document.getElementById('results').style.display = 'inline';
				document.getElementById('results').innerHTML = response.value[1];
			}
			
			
			function expand_tree(id, lvl) {
				eDirectives.Presentation.Resources.Ajax.AjaxMethods.getChildList(id, parseInt(lvl), expand_list_cb);
			}
			
			function expand_directive(id, lvl) {
				eDirectives.Presentation.Resources.Ajax.AjaxMethods.getDirCompList(id, parseInt(lvl), expand_dir_cb);
			}
			
			function expand_list_cb(response) {
				document.getElementById('lnk'+  response.value[0]).innerHTML = "<a class=\"exp_href\" href=\"javascript:OnClick=collapse_tree('"+ response.value[0] +"', "+ response.value[2] +");\"><img src=\"Images/folder_opened.png\" border=0></a>&nbsp;";
				document.getElementById('alnk'+ response.value[0]).href = "javascript:OnClick=collapse_tree('"+ response.value[0] +"', "+ response.value[2] +");";

				document.getElementById('expand'+ response.value[0]).style.display = 'inline';
				document.getElementById('expand'+ response.value[0]).innerHTML = response.value[1];
			}
			
			function expand_dir_cb(response) {
				document.getElementById('lnk'+  response.value[0]).innerHTML = "<a class=\"exp_href\" href=\"javascript:OnClick=collapse_dir('"+ response.value[0] +"', "+ response.value[2] +");\"><img src=\"Images/folder_opened.png\" border=0></a>&nbsp;";
				document.getElementById('expand'+ response.value[0]).style.display = 'inline';
				document.getElementById('expand'+ response.value[0]).innerHTML = response.value[1];
			}
			
			function show_directive (id) {
				eDirectives.Presentation.Resources.Ajax.AjaxMethods.displayDirective(id, show_directive_cb);
			}
			
			function show_directive_cb (response) {
				document.getElementById('dir_disp').style.display = 'inline';
				document.getElementById('dir_disp').innerHTML = '<table width=95% align=center><tr><td>'+ response.value +'</td></tr></table>';
				document.getElementById('results').style.display = 'none';
				document.getElementById('qs_ea').style.display = 'none';
				document.getElementById('qs_rt').style.display = 'inline';
			}
			
			function return_tree() {
				document.getElementById('dir_disp').style.display = 'none';
				document.getElementById('results').style.display = 'inline';
				document.getElementById('qs_ea').style.display = 'inline';
				document.getElementById('qs_rt').style.display = 'none';
			}
			
			function collapse_tree(id, lvl) {
				document.getElementById('expand'+ id).innerHTML = '';
				document.getElementById('expand'+ id).style.display = 'none';
				document.getElementById('lnk'+  id).innerHTML = "<a class=\"exp_href\" href=\"javascript:OnClick=expand_tree('"+ id +"', "+ lvl +");\"><img src=\"Images/folder_closed.png\" border=0></a>&nbsp;";
				document.getElementById('alnk'+ id).href = "javascript:OnClick=expand_tree('"+ id +"', "+ lvl +");";
			}
			
			function collapse_dir(id, lvl) {
				document.getElementById('expand'+ id).innerHTML = '';
				document.getElementById('expand'+ id).style.display = 'none';
				document.getElementById('lnk'+  id).innerHTML = "<a class=\"exp_href\" href=\"javascript:OnClick=expand_directive('"+ id +"', "+ lvl +");\"><img src=\"Images/folder_closed.png\" border=0></a>&nbsp;";
			}

			function show_breadcrumb(response) {
			  document.getElementById('breadcrumb').innerHTML = response.value;
			}
	      
